{
  "name": "rot13-jest-jsuice",
  "version": "1.0.0",
  "description": "Sociable tests + mocks + DI experiment",
  "main": "index.js",
  "engines": {
    "node": "16.3.0"
  },
  "scripts": {
    "audit:html": "yarn audit --level moderate --json | yarn-audit-html --output build/reports/yarn-audit-report.html",
    "docs:jsuice": "jsdoc src/jsuice --configure ./.jsdoc.json --recurse --destination build/docs/jsuice",
    "docs:sociable-jsuice": "jsdoc src/sociable-jsuice --configure ./.jsdoc.json --recurse --destination build/docs/sociable-jsuice",
    "docs": "yarn docs:jsuice && yarn docs:sociable-jsuice",
    "launch:service": "node --unhandled-rejections=strict src/serve.js",
    "launch:unit": "JSUICE_ENV=sociable jest --passWithNoTests --silent --verbose $WATCH_PARAM --config $CONFIG_FILE",
    "launch:functional": "JSUICE_ENV=functional jest --runInBand --passWithNoTests --verbose --config $CONFIG_FILE",
    "launch:functionalWithLogs": "npm-run-all --silent --aggregate-output --continue-on-error --parallel launch:functional tail:functionalLogs",
    "lint": "mkdir -p ./reports/lint && suppress-exit-code eslint --output-file ./reports/lint/eslint.out --fix $FORMAT_PARAM \"src/**/*.js\" 2>&1 | tee ./reports/lint/eslint.out",
    "tail:functionalLogs": "suppress-exit-code less +F -h0 logs/functional.out",
    "test:unit": "CONFIG_FILE=./src/conf/jest.unit.config.js yarn launch:unit",
    "test:functional": "CONFIG_FILE=./src/conf/jest.functional.config.js yarn launch:functionalWithLogs",
    "launch:watchUnit": "CONFIG_FILE=./src/conf/jest.unit.watch.js WATCH_PARAM=\"--watch\" yarn --silent --mutex file:./build/unit.mutex launch:unit | cat",
    "launch:watchLint": "chokidar \"src/**/*.js\" --initial --silent --command \"FORMAT_PARAM=--no-color yarn --silent --mutex file:./build/lint.mutex lint\"",
    "launch:watchFunctional": "chokidar \"src/**/*.js\" --initial --silent --command \"CONFIG_FILE=./src/conf/jest.functional.watch.js yarn --verbose --mutex file:./build/functional.mutex launch:functionalWithLogs | cat\"",
    "watch:unit": "mkdir -p ./build && yarn launch:watchLint & yarn launch:watchUnit",
    "watch:functional": "mkdir -p ./build && yarn launch:watchFunctional"
  },
  "author": "woldie",
  "license": "UNLICENSED",
  "dependencies": {
    "@awaitjs/express": "^0.7.2",
    "class-info": "^1.0.2",
    "dotenv-flow": "^3.2.0",
    "examine-instance": "^1.1.2",
    "express": "^4.15.2",
    "express-ajv-middleware": "^2.0.1",
    "express-list-routes": "^1.1.3",
    "grapheme-splitter": "^1.0.4",
    "http-status-codes": "^2.1.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.filter": "^4.6.0",
    "lodash.flatten": "^4.4.0",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.isarray": "^4.0.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isnumber": "^3.0.3",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.isundefined": "^3.0.1",
    "lodash.join": "^4.0.1",
    "lodash.keys": "^4.2.0",
    "lodash.map": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "lodash.trim": "^4.5.1",
    "lodash.union": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "type-check": "^0.4.0"
  },
  "devDependencies": {
    "@jest/reporters": "^27.0.5",
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/check-types": "^7.3.2",
    "@types/jest": "^26.0.23",
    "@types/jest-expect-message": "^1.0.3",
    "@types/sinonjs__fake-timers": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@typescript-eslint/types": "^4.26.1",
    "call-id": "^0.1.0",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fs-extra": "^10.0.0",
    "get-param-names": "^1.0.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-dev-server": "^5.0.3",
    "jest-expect-message": "^1.0.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-reporter": "^1.0.1",
    "jsdoc": "^3.6.7",
    "jsdoc-plugin-intersection": "^1.0.4",
    "jsdoc-typeof-plugin": "^1.0.0",
    "jsdom-global": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^11.1.1",
    "superagent": "^6.1.0",
    "suppress-exit-code": "^1.0.0",
    "testdouble": "^3.16.1",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "yarn-audit-html": "^2.0.1"
  }
}
