{
  "name": "rot13-jest-jsuice",
  "version": "1.0.0",
  "description": "Sociable tests + mocks + DI experiment",
  "main": "index.js",
  "engines": {
    "node": "16.3.0"
  },
  "scripts": {
    "audit:html": "yarn audit --level moderate --json | yarn-audit-html --output build/reports/yarn-audit-report.html",
    "build": "yarn lint && tsc",
    "docs:jsuice": "jsdoc src/jsuice --configure ./.jsdoc.json --recurse --destination build/docs/jsuice",
    "docs": "yarn docs:jsuice",
    "launch:app": "node -r ts-node/register -r tsconfig-paths/register --unhandled-rejections=strict ./src/App.ts",
    "launch:unit": "jest --passWithNoTests --silent --verbose $WATCH_PARAM --config $CONFIG_FILE",
    "launch:integration": "jest --runInBand --passWithNoTests --silent --verbose --config $CONFIG_FILE",
    "lint": "mkdir -p ./reports/lint && suppress-exit-code eslint --output-file ./reports/lint/eslint.out --fix $FORMAT_PARAM \"src/**/*.js\" 2>&1 | tee ./reports/lint/eslint.out",
    "test:unit": "CONFIG_FILE=./jest.unit.config.js yarn launch:unit",
    "launch:watchUnit": "CONFIG_FILE=./jest.unit.watch.js WATCH_PARAM=\"--watch\" yarn --silent --mutex file:./build/unit.mutex launch:unit | cat",
    "launch:watchLint": "chokidar \"src/**/*.js\" --initial --silent --command \"FORMAT_PARAM=--no-color yarn --silent --mutex file:./build/lint.mutex lint\"",
    "watch:unit": "mkdir -p ./build && yarn launch:watchLint & yarn launch:watchUnit"
  },
  "author": "woldie",
  "license": "UNLICENSED",
  "dependencies": {
    "@awaitjs/express": "^0.7.2",
    "dotenv-flow": "^3.2.0",
    "express": "^4.15.2",
    "express-ajv-middleware": "^2.0.1",
    "express-list-routes": "^1.1.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.isfunction": "^3.0.9",
    "lodash.isnumber": "^3.0.3",
    "lodash.isstring": "^4.0.1",
    "lodash.map": "^4.6.0",
    "lodash.trim": "^4.5.1",
    "lodash.union": "^4.6.0",
    "lodash.uniq": "^4.5.0"
  },
  "devDependencies": {
    "@jest/reporters": "^27.0.5",
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@typescript-eslint/types": "^4.26.1",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "get-param-names": "^1.0.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-reporter": "^1.0.1",
    "jsdoc": "^3.6.7",
    "jsdoc-plugin-intersection": "^1.0.4",
    "jsdoc-typeof-plugin": "^1.0.0",
    "jsdom-global": "^3.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^11.1.1",
    "suppress-exit-code": "^1.0.0",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "yarn-audit-html": "^2.0.1"
  }
}
